@model IEnumerable<NordicDoorWeb.Models.Kostnad>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.kostnad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.med_uten_K)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.type_K)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.kostnad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.med_uten_K)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type_K)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.kostnad_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.kostnad_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.kostnad_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
